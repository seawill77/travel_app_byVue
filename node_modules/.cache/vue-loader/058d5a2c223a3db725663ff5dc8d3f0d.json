onents: {\n    DetailBanner,\n    DetailHeader,\n    DetailList\n  },\n  data() {\n    return {\n      sightName: '',\n      bannerImg: '',\n      gallaryImgs: [],\n      list: [],\n      pageId: 0\n    }\n  },\n  methods: {\n    getDetailInfo() {\n      axios\n        .get('/api/detail.json', {\n          params: {\n            id: this.$route.params.id\n          }\n        })\n        .then(this.getDetailInfoSuccess)\n    },\n    getDetailInfoSuccess(res) {\n      res = res.data\n      if (res.ret && res.data) {\n        const data = res.data\n        this.sightName = data[this.pageId].sightName\n        this.bannerImg = data[this.pageId].bannerImg\n        this.gallaryImgs = data[this.pageId].gallaryImgs\n        this.list = data[this.pageId].priceType\n      }\n    }\n  },\n  mounted() {\n    this.getDetailInfo()\n  },\n  activated() {\n    this.getDetailInfo()\n    this.pageId = Number(this.$route.params.id) - 1\n  }\n}\n</script>\n\n<style lang=\"stylus\" scoped>\n.content\n  width 100%\n  height 20rem\n</style>"]}]}